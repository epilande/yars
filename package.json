{
  "name": "yars",
  "version": "0.0.1",
  "description": "Yet Another React Starter",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint ./src/**/*.js",
    "lint:css": "stylelint ./src/**/*.css",
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --config webpack.config.development.babel.js --host 0.0.0.0",
    "dev:dashboard": "webpack-dashboard -- webpack-dev-server --config webpack.config.development.babel.js",
    "prod": "NODE_ENV=production webpack --config webpack.config.production.babel.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 6.x",
    "npm": ">= 3.x"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sanitize.css": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.0",
    "offline-plugin": "^4.5.5",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^3.0.0",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.16.1",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-merge": "^4.0.0"
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
